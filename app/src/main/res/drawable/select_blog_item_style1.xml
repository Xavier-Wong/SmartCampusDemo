<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:right="-2dp"
        android:left="-2dp"
        android:state_pressed="true">
        <shape>
            <stroke
                android:width="1dp"
                android:color="@color/BLOG_ITEM_DIVIDER"
                android:dashWidth="10dp" />
            <solid android:color="@color/BLOG_ITEM_PRESS" />
        </shape>
    </item>
    <item
        android:right="-2dp"
        android:left="-2dp">
        <shape>
            <stroke
                android:width="1dp"
                android:color="@color/BLOG_ITEM_DIVIDER"
                android:dashWidth="10dp" />
            <solid android:color="@color/BLOG_ITEM_BG" />
        </shape>
    </item>
</selector>